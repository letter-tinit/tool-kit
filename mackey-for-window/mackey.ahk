/**
 * ! = Alt
 * ^ = Control
 * + = Shift
 * # = Windows
 */

!a::SendInput, ^a
!c::SendInput, ^c
!x::SendInput, ^x
!v::SendInput, ^v
!o::SendInput, ^o
!s::SendInput, ^s
!z::SendInput, ^z
!+z::SendInput, ^y
!f::SendInput, ^f
!r::SendInput, ^r

!#m::SendInput, #m
^Space::Send, #{Space}
F11::SendInput, #m
!#Esc::SendInput, ^+Esc
